              "align": "center",
              "displayMode": "gradient-gauge"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "blue", "value": null},
                {"color": "green", "value": 1000},
                {"color": "red", "value": 5000}
              ]
            }
          },
          "overrides": []
        },
        "options": {
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        },
        "targets": [
          {
            "datasource": "PostgreSQL",
            "rawSql": "SELECT SUM(transaction_count) as total_transactions FROM device_stats",
            "format": "table",
            "refId": "A"
          }
        ]
      },
      {
        "id": 5,
