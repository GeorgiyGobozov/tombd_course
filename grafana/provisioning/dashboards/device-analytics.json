        "options": {
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        },
        "targets": [
          {
            "datasource": "PostgreSQL",
            "rawSql": "SELECT AVG(success_rate) as avg_success_rate FROM device_stats",
